{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "aws/aws-sdk-php": "^3.279",
    "react/event-loop": "^1.4.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "SqsWorker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SqsWorker\\Tests\\": "tests/"
    }
  }
}
